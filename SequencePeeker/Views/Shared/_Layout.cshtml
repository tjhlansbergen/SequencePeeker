<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="format-detection" content="telephone=no" /> <!-- disables underlining of numbers in edge -->
    <title>@ViewBag.Title - SequencePeeker</title>
    @Styles.Render("~/styles/site.css")
</head>
<body>

    <div class="header">
        <h1>SequencePeeker</h1>
        <text>Build @(typeof(SequencePeeker.MvcApplication).Assembly.GetName().Version.Build - 6851)</text>
    </div>


    <div class="main">
        @RenderBody()
    </div>


</body>
</html>
